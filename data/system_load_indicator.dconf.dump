[general]
background-color='traditional:background'
indicator-expression-index=5
menu-expressions=['CPU: $(percent(cpu.inuse)), iowait $(percent(cpu.io))', 'Mem: $(size(mem.user)), cache $(size(mem.cached))', 'Net: down $(speed(net.down)), up $(speed(net.up))', 'Swap: $(size(swap.used))', 'Disk: read $(speed(disk.read)), write $(speed(disk.write))']
color-scheme='traditional'
width=uint16 40
indicator-expressions=['CPU $(percent(cpu.inuse))', 'Mem $(size(mem.user))', 'Net $(speed(net.down))/$(speed(net.up))', 'Swap $(size(swap.used))', 'Disk $(speed(disk.read))/$(speed(disk.write))']
settings-version=uint32 3

[graphs/cpu/cpu1]
color='traditional:cpu1'

[graphs/cpu/cpu4]
color='traditional:cpu4'

[graphs/cpu/cpu3]
color='traditional:cpu3'

[graphs/cpu/cpu2]
color='traditional:cpu2'

[graphs/swap/swap1]
color='traditional:swap1'

[graphs/swap]
enabled=false

[graphs/net/net1]
color='traditional:net1'

[graphs/net/net3]
color='traditional:net3'

[graphs/net]
enabled=true

[graphs/net/net2]
color='traditional:net2'

[graphs/disk/disk2]
color='traditional:disk2'

[graphs/disk/disk1]
color='traditional:disk1'

[graphs/disk]
enabled=true

[graphs/load/load1]
color='traditional:load1'

[graphs/load]
enabled=false

[graphs/mem/mem4]
color='traditional:mem4'

[graphs/mem/mem3]
color='traditional:mem3'

[graphs/mem/mem2]
color='traditional:mem2'

[graphs/mem]
enabled=true

[graphs/mem/mem1]
color='traditional:mem1'
